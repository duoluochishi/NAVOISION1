<UserControl
    x:Class="NV.CT.UI.Exam.View.LoadStaticDicomControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="650"  d:DesignWidth="1450"  mc:Ignorable="d">
    <StackPanel Orientation="Horizontal">
        <ContentControl Width="726" Content="{Binding LeftWindowsFormsHost, Mode=TwoWay}" />
        <Border Width="10" CornerRadius="0" />
        <ContentControl  Width="726"  Content="{Binding RightWindowsFormsHost, Mode=TwoWay}" />
    </StackPanel>
</UserControl>