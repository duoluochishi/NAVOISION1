<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>$(TargetFramework)-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWPF>true</UseWPF>
		<StartupObject>NV.CT.Examination.Program</StartupObject>
		<UseWindowsForms>true</UseWindowsForms>
		<PlatformTarget>x64</PlatformTarget>
		<Platforms>AnyCPU;x64</Platforms>
		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
	  <NoWarn>1701;1702;CA1416</NoWarn>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
	  <NoWarn>1701;1702;CA1416</NoWarn>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
	  <NoWarn>1701;1702;CA1416</NoWarn>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
	  <NoWarn>1701;1702;CA1416</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="View\Chinese\**" />
		<EmbeddedResource Remove="View\Chinese\**" />
		<None Remove="View\Chinese\**" />
		<Page Remove="View\Chinese\**" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Autofac" />
		<PackageReference Include="Autofac.Extensions.DependencyInjection" />
		<PackageReference Include="AutoMapper" />
		<PackageReference Include="Microsoft.Extensions.Configuration" />
		<PackageReference Include="Microsoft.Extensions.Hosting" />
		<PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" />
		<PackageReference Include="Newtonsoft.Json" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\..\Common\UI.Controls\NV.CT.UI.Controls.csproj" />
		<ProjectReference Include="..\..\..\Common\ViewModel\NV.CT.UI.ViewModel.csproj" />
		<ProjectReference Include="..\..\..\SystemInterface\MCSRuntime.Impl\NV.CT.SystemInterface.MCSRuntime.Impl.csproj" />
		<ProjectReference Include="..\..\..\SystemInterface\MRSIntegration.Impl\NV.CT.SystemInterface.MRSIntegration.Impl.csproj" />
		<ProjectReference Include="..\..\..\SystemService\ClientProxy\NV.CT.ClientProxy.csproj" />
		<ProjectReference Include="..\..\..\SystemService\ProtocolService.Contract\NV.CT.ProtocolService.Contract.csproj" />
		<ProjectReference Include="..\ApplicationService.Contract\NV.CT.Examination.ApplicationService.Contract.csproj" />
		<ProjectReference Include="..\ApplicationService.Impl\NV.CT.Examination.ApplicationService.Impl.csproj" />
		<ProjectReference Include="..\UI.Exam\NV.CT.UI.Exam.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Page Update="View\MainWindow.xaml">
			<XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
		</Page>
		<Page Update="Layout\ProtocolSelectionControl.xaml">
			<XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
		</Page>
		<Page Update="View\ScanMainControl.xaml">
			<XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
		</Page>
		<Page Update="Layout\ScanDefaultControl.xaml">
			<XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
		</Page>
	</ItemGroup>

	<ItemGroup>
		<Reference Include="NV.MPS.Communication">
			<HintPath>..\..\..\bin\NV.MPS.Communication.dll</HintPath>
		</Reference>
		<Reference Include="NV.CT.FacadeProxy.Common">
			<HintPath>..\..\..\bin\NV.CT.FacadeProxy.Common.dll</HintPath>
		</Reference>
		<Reference Include="NV.MPS.Configuration">
		  <HintPath>..\..\..\bin\NV.MPS.Configuration.dll</HintPath>
		</Reference>
		<Reference Include="NV.MPS.Environment">
			<HintPath>..\..\..\bin\NV.MPS.Environment.dll</HintPath>
		</Reference>
		<Reference Include="MaterialDesignColors">
			<HintPath>..\..\..\bin\MaterialDesignColors.dll</HintPath>
		</Reference>
		<Reference Include="MaterialDesignThemes.Wpf">
			<HintPath>..\..\..\bin\MaterialDesignThemes.Wpf.dll</HintPath>
		</Reference>
		<Reference Include="NV.CT.Language">
		  <HintPath>..\..\..\bin\NV.CT.Language.dll</HintPath>
		</Reference>
		<Reference Include="NV.MPS.UI.Dialog">
			<HintPath>..\..\..\bin\NV.MPS.UI.Dialog.dll</HintPath>
		</Reference>
		<Reference Include="NV.CT.Alg.ScanReconCalculation">
			<HintPath>..\..\..\bin\NV.CT.Alg.ScanReconCalculation.dll</HintPath>
