<UserControl
	x:Class="NV.CT.NanoConsole.View.English.HeaderControl"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:NV.CT.NanoConsole.View.English"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	d:DesignHeight="40"
	d:DesignWidth="1600"
	mc:Ignorable="d">
	<!--Background="#16162B"-->
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="275" />
			<ColumnDefinition Width="70" />
			<ColumnDefinition Width="100" />
			<ColumnDefinition Width="100" />
			<ColumnDefinition Width="*" />
			<ColumnDefinition Width="300" />
			<ColumnDefinition Width="10" />
		</Grid.ColumnDefinitions>
		<local:CopyrightControl Grid.Column="0" />
		<local:FrontendFixedItemsControl Grid.Column="1" />
		<local:ScanListControl Grid.Column="2" />
		<local:DynamicCardsControl Grid.Column="4" />
		<local:BackendFixedItemsControl Grid.Column="5" />
	</Grid>
</UserControl>
