<UserControl x:Class="NV.CT.Service.Upgrade.UpgradeWrapper"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NV.CT.Service.Upgrade"
             xmlns:views="clr-namespace:NV.CT.Service.Upgrade.Views"
             xmlns:viewModels="clr-namespace:NV.CT.Service.Upgrade.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Border Style="{StaticResource WindowsBorder}" CornerRadius="10">
        <views:UpgradeView x:Name="UpgradeView" Margin="5"/>
    </Border>
</UserControl>
