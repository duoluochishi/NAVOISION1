<Window x:Class="NV.CT.Service.Upgrade.Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:upgrade="clr-namespace:NV.CT.Service.Upgrade;assembly=NV.CT.Service.Upgrade"
        xmlns:local="clr-namespace:NV.CT.Service.Upgrade.Demo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Background="Transparent"
        WindowState="Maximized">
    <Border Background="#14142B" BorderBrush="{StaticResource BaseBackgroundColor}" CornerRadius="10" >
        <Grid>
            <upgrade:UpgradeWrapper/>
        </Grid>
    </Border>
</Window>
