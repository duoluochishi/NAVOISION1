<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>$(TargetFramework)-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <Platforms>AnyCPU;x64</Platforms>
    <OutDir>..\..\..\Bin</OutDir>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" />
    <PackageReference Include="Autofac.Extensions.DependencyInjection" />
    <PackageReference Include="AutoMapper" />
    <PackageReference Include="CommunityToolkit.Mvvm" />
    <PackageReference Include="Microsoft.Extensions.Hosting" />
    <PackageReference Include="Prism.Core" />
    <PackageReference Include="Microsoft.Extensions.Logging" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="NV.CT.ErrorCodes">
      <HintPath>..\..\..\Bin\NV.CT.ErrorCodes.dll</HintPath>
    </Reference>
    <Reference Include="NV.CT.Logging">
      <HintPath>..\..\..\Bin\NV.CT.Logging.dll</HintPath>
    </Reference>
    <Reference Include="NV.CT.FacadeProxy">
      <HintPath>..\..\..\Bin\NV.CT.FacadeProxy.dll</HintPath>
    </Reference>
    <Reference Include="NV.CT.FacadeProxy.Common">
      <HintPath>..\..\..\Bin\NV.CT.FacadeProxy.Common.dll</HintPath>
    </Reference>
    <Reference Include="NV.CT.ServiceFramework">
      <HintPath>..\..\..\Bin\NV.CT.ServiceFramework.dll</HintPath>
    </Reference>
    <Reference Include="NV.MPS.Configuration">
      <HintPath>..\..\..\Bin\NV.MPS.Configuration.dll</HintPath>
    </Reference>
    <Reference Include="NV.MPS.UI.Dialog">
      <HintPath>..\..\..\Bin\NV.MPS.UI.Dialog.dll</HintPath>
    </Reference>
    <Reference Include="NV.MPS.Environment">
      <HintPath>..\..\..\Bin\NV.MPS.Environment.dll</HintPath>
    </Reference>
    <Reference Include="RawDataDel">
      <HintPath>..\..\..\Bin\RawDataDel.dll</HintPath>
    </Reference>
    <Reference Include="RawDataTools">
      <HintPath>..\..\..\Bin\RawDataTools.dll</HintPath>
    </Reference>
    <Reference Include="NV.CT.Alg.ScanReconCalculation">
      <HintPath>..\..\..\Bin\NV.CT.Alg.ScanReconCalculation.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources\Common_Lang.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Common_Lang.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Config_Lang.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Config_Lang.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Calibration_Lang.Designer.cs">
      <DependentUpon>Calibration_Lang.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Resources\HardwareTest_Lang.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>HardwareTest_Lang.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Quality_Lang.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Quality_Lang.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Upgrade_Lang.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Upgrade_Lang.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Warmup_Lang.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Warmup_Lang.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources\Common_Lang.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Common_Lang.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Config_Lang.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Config_Lang.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Calibration_Lang.resx">
      <LastGenOutput>Calibration_Lang.Designer.cs</LastGenOutput>
      <Generator>PublicResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\HardwareTest_Lang.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>HardwareTest_Lang.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Quality_Lang.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Quality_Lang.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Upgrade_Lang.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Upgrade_Lang.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Warmup_Lang.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Warmup_Lang.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>